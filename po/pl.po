#: Legend_of_Wesmere/scenarios/28_Shadows.cfg:220
msgid "Nfrigt"
msgstr ""

#. unit: description=Rkrin, type=Revenant
#: Legend_of_Wesmere/scenarios/28_Shadows.cfg:228
msgid "Rkrin"
msgstr ""

#. message: description=Rudimil
#: Legend_of_Wesmere/scenarios/28_Shadows.cfg:236
msgid "We have more help!"
msgstr "Dostaliśmy pomoc!"

#. unit: description=Haunt, type=Spectre
#: Legend_of_Wesmere/scenarios/28_Shadows.cfg:244
msgid "Haunt"
msgstr ""

#. unit: description=Gdal, type=Spectre
#: Legend_of_Wesmere/scenarios/28_Shadows.cfg:252
